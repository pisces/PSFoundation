CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/PSFoundation"
LIBRARY_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/PSFoundation"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/PSFoundation"
OTHER_LDFLAGS = $(inherited) -ObjC -l"PSFoundation" -l"sqlite3" -l"z" -framework "Foundation"
PODS_ROOT = ${SRCROOT}/Pods
